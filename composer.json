{
    "name": "faridlab/salt-laravel-countries",
    "description": "",
    "keywords": ["Laravel"],
    "authors": [{
        "name": "Farid Hidayat",
        "email": "farid@startapp.id"
    }],
    "homepage": "https://github.com/faridlab/salt-laravel-countries",
    "require": {
        "php": "^8.0.2",
        "laravel/framework": "^10.0",
        "doctrine/dbal": "^3.3",
        "faridlab/salt-laravel": "^10",
        "zircote/swagger-php": "^4.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SaltCountries\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SaltCountries\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SaltCountries\\Providers\\SaltCountriesServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit -c ./phpunit.xml --colors=always",
        "analysis": "vendor/bin/phpstan analyse"
    }
}
